    <title>{{ page_title }}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="{{ page_description | escape }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {{ content_for_header }}<!-- Header hook for plugins -->
    <!--Proto Theme Developed by Laiqa Mohid and Hasaan Latif  -->{{ 'application.css' | asset_url | stylesheet_tag }}<script defer src="{{ 'theme.js' | asset_url }}"> </script>
    {{ 'global.css' | asset_url | stylesheet_tag }}
    {{ 'product.css' | asset_url | stylesheet_tag }}
    {{ 'helpers.css' | asset_url | stylesheet_tag }}
    {{ 'flickity.css' | asset_url | stylesheet_tag }}
    {{ 'flickity.js' | asset_url | script_tag }}
    <script
      async
      type="text/javascript"
      src="https://static.klaviyo.com/onsite/js/klaviyo.js?company_id=SBNcmD"></script>
    <script defer src="https://cdn.jsdelivr.net/npm/alpinejs@3.x.x/dist/cdn.min.js"></script>
    <script src="{{ 'application.js' | asset_url }}" defer></script>
    
  </head>
  <script async src="https://www.googletagmanager.com/gtag/js?id=G-L00WSLZL4P"></script>
  <script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-L00WSLZL4P');
  </script>
  <body class="template-{{ template | replace: '.', '-' | replace: '/', '-' }}">
    <div
      id="ajaxCardContainerMobile"
      data-ajax-popup
      class="addedProduct"></div>
    <div class="wire left"></div>
    <div class="wire right"></div>
    {% section 'header' %}
    {% section 'cart' %}
    <main class="content" role="main">
      {{ content_for_layout }}
      {% section 'footer' %}
    </main>
  </body>
</html>